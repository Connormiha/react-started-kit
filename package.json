{
  "name": "react-started-kit",
  "description": "Empty project for Single Page Application. Stack technologies: Webpack, React, Redux, Mocha+Chai+Sinon, PostCSS",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Mikhail Bodrov <connormiha1@gmail.com>",
  "engines": {
    "node": ">=5.2.0",
    "npm": ">=3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Connormiha/react-started-kit.git"
  },
  "scripts": {
    "build": "NODE_ENV=production node node_modules/webpack/bin/webpack",
    "lint": "eslint src test --ext .js --ext .jsx",
    "start": "webpack-dev-server",
    "test": "NODE_PATH=$NODE_PATH:$PWD/src mocha $(find src -name '*.test.js')",
    "coverage": "NODE_PATH=$NODE_PATH:$PWD/src babel-node ./node_modules/.bin/isparta cover _mocha -- $(find src -name '*.test.js')"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react": "^1.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.12.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "ghooks": "1.2.3",
    "gulp": "^3.9.1",
    "gulp-stylint": "^3.0.0",
    "html-webpack-plugin": "2.19.0",
    "isparta": "^4.0.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.9.1",
    "react": "15.1.0",
    "react-addons-test-utils": "15.1.0",
    "react-dom": "15.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.1",
    "redux": "3.5.2",
    "seamless-immutable": "^6.1.0",
    "sinon": "^1.17.4",
    "style-loader": "0.13.1",
    "sugarss": "^0.1.4",
    "webpack": "1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-sources": "0.1.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "NODE_ENV=pre_commit npm run lint",
      "pre-push": "npm test"
    }
  }
}
