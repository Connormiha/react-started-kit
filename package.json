{
  "name": "react-started-kit",
  "description": "Empty project for Single Page Application. Stack technologies: Webpack, React, Redux, Mocha+Chai+Sinon, PostCSS",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Mikhail Bodrov <connormiha1@gmail.com>",
  "engines": {
    "node": ">=5.2.0",
    "npm": ">=3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Connormiha/react-started-kit.git"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "lint": "gulp lint",
    "start": "webpack-dev-server",
    "test": "NODE_PATH=$NODE_PATH:$PWD/src mocha $(find src -name '*.test.js')",
    "coverage": "NODE_PATH=$NODE_PATH:$PWD/src babel-node ./node_modules/.bin/isparta cover _mocha -- $(find src -name '*.test.js')"
  },
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react": "1.0.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "bem-cn": "2.1.2",
    "chai": "3.5.0",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.0.1",
    "eslint-loader": "1.4.1",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-react": "5.2.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "ghooks": "1.3.2",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-stylint": "^3.0.0",
    "html-webpack-plugin": "2.22.0",
    "ignore-styles": "^4.0.0",
    "isparta": "4.0.0",
    "json-loader": "0.5.4",
    "lodash": "^4.13.1",
    "mocha": "2.5.3",
    "postcss": "5.0.21",
    "poststylus": "^0.2.3",
    "react": "15.2.1",
    "react-addons-test-utils": "15.2.1",
    "react-dom": "15.2.1",
    "react-redux": "4.4.5",
    "react-router": "2.5.2",
    "redux": "3.5.2",
    "seamless-immutable": "6.1.1",
    "sinon": "1.17.4",
    "style-loader": "0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1",
    "webpack-notifier": "^1.3.1",
    "webpack-sources": "0.1.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "NODE_ENV=pre_commit npm run lint",
      "pre-push": "npm test"
    }
  }
}
